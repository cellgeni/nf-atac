process {
    withName: '.*ANNDATA_TOH5AD' {
        ext.args   = ''
        cpus       = 1
        queue      = 'normal'
        memory     = { 10.GB + 30.GB * (task.attempt - 1) }
        publishDir = [
            mode     : 'link',
            path     : 'results',
            overwrite: true
        ]
    }
}