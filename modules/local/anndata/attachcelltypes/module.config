process {
    withName: '.*ANNDATA_ATTACHCELLTYPES' {
        ext.barcode_column = 'obs_names'
        ext.output = ''
        cpus       = 1
        queue      = 'normal'
        memory     = { 10.GB + 30.GB * (task.attempt - 1) }
        publishDir = [
            mode     : 'link',
            path     : 'results',
            overwrite: true
        ]
    }
}